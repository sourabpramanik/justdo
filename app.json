{
  "expo": {
    "name": "Viron",
    "slug": "viron",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thexenon.viron",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.thexenon.viron",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"        
      }
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }    
  }
}
