{
  "expo": {
    "name": "JustDo",
    "slug": "justdo",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/justdo-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thexenon.justdo",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.thexenon.justdo",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/justdo-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}